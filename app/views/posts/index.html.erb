<p id="notice"><%= notice %></p>

<div align='center'>
<h1>Instagram</h1>

<%= link_to 'New Post', new_post_path %><br>
&nbsp;<br>
<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <%= link_to image_tag(post.image), post %> <br>
        <%= post.message %>
        <br>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
